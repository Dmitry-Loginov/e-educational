<form method="post" class="form-del" action="{{ path('subject_delete', {'id': subject.id}) }}" onsubmit="return confirm('Удалить предмет?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ subject.id) }}">
    <button class="btn btn-outline-secondary">Удалить</button>
</form>
