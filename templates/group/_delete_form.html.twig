<form method="post" action="{{ path('group_delete', {'id': group.id}) }}" onsubmit="return confirm('Вы уверены, что хотите удалить эту группу?');" class="m-1">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ group.id) }}">
    <button class="btn btn-outline-danger btn-sm">Удалить</button>
</form>